<included>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} [%highlight(%-5level)] [%32X{traceId:-},%16X{spanId:-}] %-40.40logger{39} | %msg%n%wEx</pattern>
            <charset>utf8</charset>
        </encoder>
    </appender>
</included>